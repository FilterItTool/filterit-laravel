{
  "name": "filterit/filterit-laravel",
  "description": "Laravel FilterIt is a fully customizable package for parsing query strings into builder query.",
  "type": "package",
  "license": "MIT",
  "homepage": "https://github.com/FilterIt/filterit-laravel",
  "keywords": [
    "laravel",
    "filterit",
    "filter"
  ],
  "authors": [
    {
      "name": "Emad Malekpour",
      "email": "prodemmi@gmail.com",
      "homepage": "https://github.com/prodemmi"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1@dev",
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "FilterIt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FilterIt\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FilterItServiceProvider"
      ],
      "aliases": {
        "FilterIt": "FilterIt\\Facades\\FilterIt"
      }
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "minimum-stability": "dev"
}
